{
  "name": "it-ops-backend",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --no-cache src/tests/debug.test.ts",
    "test:clear": "jest --clearCache",
    "test:runner": "node src/tests/runner.js",
    "test:custom": "node src/tests/runtests.js",
    "test:all": "npm run test:clear && jest --coverage --detectOpenHandles --forceExit",
    "test:ci": "jest --ci --runInBand --coverage --reporters=default --reporters=jest-junit",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "dependencies": {
    "@azure/identity": "^4.7.0",
    "@microsoft/microsoft-graph-client": "^3.0.7",
    "@slack/web-api": "^7.8.0",
    "activedirectory2": "^2.2.0",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "express-validator": "^7.2.1",
    "helmet": "^7.2.0",
    "ioredis": "^5.5.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.2.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.10.0",
    "prom-client": "^15.1.3",
    "redis": "^4.6.6",
    "systeminformation": "^5.25.11",
    "uuid": "^11.0.5",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mongodb": "^4.0.7",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.17.19",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "cross-env": "^7.0.3",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
