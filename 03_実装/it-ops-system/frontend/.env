# APIエンドポイント設定
REACT_APP_API_URL=http://localhost:3000/api
PORT=3001

# 認証設定
REACT_APP_AUTH_STORAGE_KEY=it_ops_token

# アプリケーション設定
REACT_APP_TITLE=IT運用システム
REACT_APP_VERSION=1.0.0

# メトリクス更新間隔（ミリ秒）
REACT_APP_METRICS_UPDATE_INTERVAL=30000

# ログ設定
REACT_APP_MAX_LOGS_DISPLAY=1000

# アラート設定
REACT_APP_MAX_ALERTS_DISPLAY=100

# デバッグモード
REACT_APP_DEBUG=true